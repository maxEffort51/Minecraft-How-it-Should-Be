	"environment": "*",
	"entrypoints": {
		"main": [
			"net.aaronterry.hisb.main.HisbMod"
		],
		"client": [
			"net.aaronterry.hisb.main.HisbModClient"
		],
		"fabric-datagen": [
			"net.aaronterry.hisb.main.HisbModDataGenerator"
		]
	},
	"mixins": [
		"exploration.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.7",
