{
	"schemaVersion": 1,
	"id": "hisb",
	"version": "${version}",
	"name": "Minecraft How it Should Be - 1.21",
	"description": "The mod to fix Minecraft's problems, from adventure to building to redstone!",
	"authors": [
		"Aaron Terry"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
